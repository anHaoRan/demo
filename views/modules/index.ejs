<body>
    <div id="app">
        <h1>网站建设中。。。。</h1>
        <el-button @click="visible = true">Button22222</el-button>
        <el-button @click="restart">重新部署</el-button>
        <el-dialog :visible.sync="visible" title="Hello world">
            <p>Try Element</p>
        </el-dialog>
    </div>
</body>
<%- include ../common/common-js.ejs %>
    <script>
        new Vue({
            el: '#app',
            data: function () {
                return {
                    visible: false
                }
            },
            methods: {
                restart: function () {
                    this.$http.get('/upload')
                        .then(function (resp) {
                            location.reload();
                        });
                }
            },
            ready:function(){
                this.restart();
            }
        })
    </script>